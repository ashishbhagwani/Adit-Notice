<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/AppTheme"
    android:background="@android:color/white"
    tools:context="com.ashish.adit_portal.Fragments.FeedbackForm">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:id="@+id/enrollment_layout">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="17sp"
                android:text="@string/enrollment_number"
                android:id="@+id/Enrollment_feed"
                android:layout_marginStart="5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_centerVertical="true"
                tools:text="140010107006"
                android:textSize="17sp"
                android:layout_toEndOf="@id/Enrollment_feed"
                android:id="@+id/enrollment_text_feed"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="@string/hide"
                android:layout_marginEnd="10dp"
                android:layout_alignParentEnd="true"
                android:textColor="@android:color/white"
                android:background="@drawable/button"
                android:id="@+id/hide_button"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_below="@id/enrollment_layout"
            android:id="@+id/subject_code_layout">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="18sp"
                android:text="@string/subject_code"
                android:id="@+id/subject_code"
                android:layout_marginStart="5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_centerVertical="true"
                tools:text="2150703"
                android:textSize="18sp"
                android:layout_toEndOf="@id/subject_code"
                android:id="@+id/subject_code_text"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_below="@id/subject_code_layout"
            android:id="@+id/Subject_layout"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="18sp"
                android:layout_marginStart="5dp"
                android:text="@string/subject"
                android:id="@+id/Subject_feed"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_centerVertical="true"
                tools:text="Analyis and design of algorithm"
                android:textSize="18sp"
                android:layout_toEndOf="@id/Subject_feed"
                android:id="@+id/Subject_text_feed"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_below="@id/Subject_layout"
            android:id="@+id/Faculty_layout"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="18sp"
                android:layout_marginStart="5dp"
                android:text="@string/faculty"
                android:id="@+id/Faculty_feed"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_centerVertical="true"
                tools:text="Priyanka Punvar"
                android:textSize="18sp"
                android:layout_toEndOf="@id/Faculty_feed"
                android:id="@+id/Faculty_text_feed"/>
        </RelativeLayout>
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/Faculty_layout"
            android:layout_marginTop="10dp"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:text="@string/start_filling_button"
            android:id="@+id/start_filling"
            android:textColor="@android:color/white"
            android:background="@drawable/button"/>
    </RelativeLayout>
</FrameLayout>
